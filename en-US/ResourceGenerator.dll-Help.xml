    <command:details>
      <command:name>New-ResourceFile</command:name>
      <command:verb>New</command:verb>
      <command:noun>ResourceFile</command:noun>
      <maml:description>
        <maml:para>Creates resources for .NET application and assemblies.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>The `New-ResourceFile` cmdlet creates resources for .NET application and assemblies.</maml:para>
    </maml:description>
