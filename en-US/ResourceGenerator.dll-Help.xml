    <command:examples>
      <command:example>
        <maml:title>-------------------------- Example 1 --------------------------</maml:title>
        <dev:code>PS C:\&gt; New-ResourceFile -ResourceList @{ HelloWorld = 'Hello World!' } -OutputDirectory 'C:\Projects' -Namespace 'HelloWorld' -TypeName 'Resources'</dev:code>
        <dev:remarks>
          <maml:para>This example creates `Resources.resx` and `Resources.Designer.cs` files in the specified directory `C:\Projects`.</maml:para>
        </dev:remarks>
      </command:example>
      <command:example>
        <maml:title>-------------------------- Example 2 --------------------------</maml:title>
        <dev:code>PS C:\&gt; New-ResourceFile -Path 'C:\Projects\Resources.txt' -OutputDirectory 'C:\Projects' -Namespace 'HelloWorld' -TypeName 'Resources' -ResourceType All</dev:code>
        <dev:remarks>
          <maml:para>This example creates `Resources.resx`, `Resources.Designer.cs`, and `Resources.resources` files in the specified directory `C:\Projects`.</maml:para>
        </dev:remarks>
      </command:example>
